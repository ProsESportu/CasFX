<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import com.sun.javafx.scene.control.DoubleField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo2.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <TextField fx:id="money" text="enter your money here"/>
    <Label fx:id="result" text="Hello, JavaFX!"/>
    <Button text="pull" onAction="#onPullButtonClick"/>
    <Label fx:id="message"/>
    <Button text="quit" onAction="#onQuitButtonClick"/>
</VBox>
